getgenv().Shooter = true
getgenv().Timer = 0.5075
getgenv().holding = false
getgenv().AutoGuard = true
getgenv().Distance = 5.5
getgenv().Guarding = false
getgenv().Shooting = false
getgenv().Block = true
getgenv().Special = false
local holdkey = {}
local ids = {}
local playc = {}
local playerl = {}
local idsh = {}
local Spoofs = {}
function spoof(instance,property)
if not Spoofs[instance] or #Spoofs[instance] == 0 then
   for i,v in pairs(getconnections(instance.Changed)) do
   v:Disable()
   end
   for i,v in pairs(getconnections(instance.Parent.DescendantRemoving)) do
   v:Disable()
   end
   for i,v in pairs(getconnections(instance.Parent.ChildRemoved)) do
   v:Disable()
   end
if property ~= nil then
for i,v in pairs(getconnections(instance:GetPropertyChangedSignal(property))) do
   v:Disable()
end
local newvalueg, newvaluee = instance[property], instance[property]
Spoofs[instance] = {property}
Spoofs[instance][property] = {newvalueg,newvaluee}
     end
  end
end
function unspoof(instance,property)
   if property ~= nil then
   for i,v in pairs(getconnections(instance:GetPropertyChangedSignal(property))) do
   v:Enable()
   end
   for i,v in pairs(Spoofs[instance]) do
	   if v == property then
	   table.remove(Spoofs[instance],i)
	   end
   end
end
   for i,v in pairs(getconnections(instance.Changed)) do
   v:Enable()
   end
   for i,v in pairs(getconnections(instance.Parent.DescendantRemoving)) do
   v:Enable()
   end
   for i,v in pairs(getconnections(instance.Parent.ChildRemoved)) do
   v:Enable()
   end
end
local h;
h = hookmetamethod(game,'__newindex',function(self,prop,new) 
if Spoofs[self] and Spoofs[self][prop] and not checkcaller() then
Spoofs[self][prop][1] = new
new = Spoofs[self][prop][2]
elseif Spoofs[self] and Spoofs[self][prop] and checkcaller() then
Spoofs[self][prop][2] = new
end
return h(self,prop,new)
end)
local h;
h = hookmetamethod(game,'__index',function(self,...) 
if Spoofs[self] and Spoofs[self][...] and not checkcaller() then
	return Spoofs[self][...][1]
end
return h(self,...)
end)
task.spawn(function()
while true do task.wait()
if game.Players.LocalPlayer.Character then
spoof(game.Players.LocalPlayer.Character.Humanoid,"WalkSpeed")
game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 12
end
end
end)
local h;
h = hookmetamethod(game,'__namecall', function(self,...)
local args = {...}
if tostring(self) == "ClientAction" and getnamecallmethod() == "FireServer" and args[1] == "Dribble Move" then
task.spawn(function()
getgenv().Special = true
getgenv().Timer = 0.65
task.wait(3.5)
getgenv().Special = false
end)
return self.FireServer(self,unpack(args))
elseif tostring(self) == "ClientAction" and getnamecallmethod() == "FireServer" and args[1] == "Guard" then
getgenv().Guarding = args[2]
end
return h(self,...) 
end)
task.spawn(function()
while true do task.wait()
for i,v in pairs(game.Players:GetPlayers()) do
if v ~= game.Players.LocalPlayer and v.Character and not table.find(playc, v.Character) then
table.insert(playc,v.Character)
local b = Instance.new("Part", v.Character)
b.Size = Vector3.new(0, 0, 0)
b.Name = "Contesting"
b.Anchored = true
b.CanCollide = false
task.spawn(function()
while task.wait() do
if b and game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:WaitForChild("OpponentHoop",9e9).Value ~= nil then
b.CFrame = CFrame.lookAt(v.Character:WaitForChild("HumanoidRootPart",9e9).Position, workspace:WaitForChild(tostring(game.Players.LocalPlayer.Character:WaitForChild("OpponentHoop",9e9).Value),9e9).Goal.Position - Vector3.new(0, 5.791165828704834, 0))
  end
 end
end)
      end	
    end
  end
end)
task.spawn(function()
while true do task.wait()
for i,v in pairs(game.Players:GetPlayers()) do
if v ~= game.Players.LocalPlayer and game.Players.LocalPlayer.Character and v.Character and v.Character:FindFirstChild("ball.weld") and getgenv().AutoGuard == true and getgenv().Guarding == true then
game.Players.LocalPlayer.Character:WaitForChild("Humanoid",9e9):MoveTo(v.Character.Contesting.Position + (v.Character.Contesting.CFrame.lookVector * getgenv().Distance))
      end
    end
  end
end)
task.spawn(function()
while task.wait(0.1) do
task.spawn(function()
for i,v in pairs(game.Players:GetPlayers()) do
if v ~= game.Players.LocalPlayer and v.TeamColor ~= game.Players.LocalPlayer.TeamColor and v.Character and not table.find(playerl,v.Character) then
table.insert(playerl,v.Character)
task.spawn(function()
v.Character.ChildAdded:Connect(function(b)
if b.Name == "ShotMeterTiming" and getgenv().Block == true and game.Players.LocalPlayer:DistanceFromCharacter(v.Character:WaitForChild("HumanoidRootPart",9e9).Position) <= 19 then
getgenv().Distance = 0
task.wait()
game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer("Jump")
getgenv().Distance = 5.5
task.wait(5)
end
end)
end)
end
end
end)
end
end)
task.spawn(function()
local character = game.Players.LocalPlayer.Character or game.Players.LocalPlayer.CharacterAdded:Wait()
for i,v in pairs(game.Players.LocalPlayer.Character:WaitForChild("Humanoid",9e9).Animations:GetChildren()) do
	if v:IsA("Animation") and v.Name:match("Dunk") and not table.find(ids,v.AnimationId) then
	table.insert(ids,v.AnimationId)
	end
end
for i,v in pairs(game.Players.LocalPlayer.Character:WaitForChild("Humanoid",9e9).Animations:GetChildren()) do
	if v:IsA("Animation") and v.Name:match("step") and not table.find(idsh,v.AnimationId) then
    table.insert(idsh,v.AnimationId)
	    end
     end
game.Players.LocalPlayer.Character:WaitForChild("Humanoid",9e9).AnimationPlayed:Connect(function(Anim)
for i,v in pairs(idsh) do
if Anim.Animation.AnimationId == v and (game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart",9e9).Position - workspace:WaitForChild(tostring(game.Players.LocalPlayer.Character:WaitForChild("MyHoop",9e9).Value),9e9).Goal.Position).Magnitude <= 18 then
getgenv().Special = true
getgenv().Timer = 0.445
task.wait(3.5)
getgenv().Special = false
elseif Anim.Animation.AnimationId == v and (game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart",9e9).Position - workspace:WaitForChild(tostring(game.Players.LocalPlayer.Character:WaitForChild("MyHoop",9e9).Value),9e9).Goal.Position).Magnitude > 18 then
getgenv().Special = true
getgenv().Timer = 0.6
task.wait(3.5)
getgenv().Special = false
    end
end
for i,v in pairs(ids) do
if Anim.Animation.AnimationId == v and getgenv().Special == false then
getgenv().Special = true
getgenv().Timer = 0.545
task.wait(3.5)
getgenv().Special = false
    end
  end
 end)
end)
task.spawn(function()
while true do
if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character:FindFirstChild('ShotMeterTiming') and getgenv().Shooter == true then
getgenv().Shooting = true
task.wait(getgenv().Timer - (game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValue() * 0.001))
game:GetService("ReplicatedStorage").GameEvents.ClientAction:FireServer("Shoot",false)
getgenv().Shooting = false
getgenv().holding = false
       end
	   task.wait()
     end
end)
game:GetService("RunService").RenderStepped:Connect(function()
task.spawn(function()
if game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Magnitude > 0 then
getgenv().holding = true
elseif game.Players.LocalPlayer.Character and game.Players.LocalPlayer.Character.Humanoid.MoveDirection.Magnitude == 0 then
getgenv().holding = false
end
end)
end)
game:GetService("RunService").RenderStepped:Connect(function()
task.spawn(function()
if getgenv().holding == true and getgenv().Special == false and (game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart",9e9).Position - workspace:WaitForChild(tostring(game.Players.LocalPlayer.Character:WaitForChild("MyHoop",9e9).Value),9e9).Goal.Position).Magnitude > 18 then
getgenv().Timer = 0.6545
end
end)
end)
game:GetService("RunService").RenderStepped:Connect(function()
task.spawn(function()
if getgenv().holding == false and getgenv().Special == false and game.Players.LocalPlayer.Character and (game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart",9e9).Position - workspace:WaitForChild(tostring(game.Players.LocalPlayer.Character:WaitForChild("MyHoop",9e9).Value),9e9).Goal.Position).Magnitude > 18 then
getgenv().Timer = 0.6
      end
end)
end)
game:GetService("RunService").RenderStepped:Connect(function()
task.spawn(function()
if game.Players.LocalPlayer.Character and getgenv().Special == false and (game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart",9e9).Position - workspace:WaitForChild(tostring(game.Players.LocalPlayer.Character:WaitForChild("MyHoop",9e9).Value),9e9).Goal.Position).Magnitude <= 18 then
getgenv().Timer = 0.5145
      end
end)
end)
task.spawn(function()
for i,v in pairs(workspace:GetDescendants()) do
	if v.Name == "Net" or v.Name == "Stand" or v.Name == "Lights" then
	v:Destroy()
	end
end
workspace:FindFirstChild("Arena"):Destroy()
game.ReplicatedStorage:FindFirstChild("People"):Destroy()
end)
