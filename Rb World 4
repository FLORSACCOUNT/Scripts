  			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Spoofs[game.Players.LocalPlayer.Character.Humanoid][2]
			unspoof(game.Players.LocalPlayer.Character.Humanoid,"WalkSpeed")
	end
end)
